<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= user.phone %>
  </p>

  <p>
    <strong>Token:</strong>
    <%= user.token %>
  </p>

  <p>
    <strong>Confirmed:</strong>
    <%= user.confirmed %>
  </p>

  <p>
    <strong>Answered:</strong>
    <%= user.answered %>
  </p>

</div>
